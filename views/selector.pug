extends layout

block content
  h1= title
  - let image_names = ['fan1.jpg','fan2.jpg','fan3.jpg','fan4.jpg','fan5.jpg']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]

      img(src=`images/${iname}` alt='fan' style='width:300px; height:300px;')
      |    
  - }